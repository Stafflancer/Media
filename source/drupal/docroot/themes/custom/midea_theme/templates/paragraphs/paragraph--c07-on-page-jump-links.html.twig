{% set navList = [] %}
{% for i in content.field_c07_items['#items']|keys %}
  {% set item = {
        "copy": content.field_c07_items[i]['#paragraph'].field_c07_item_title.getValue()[0]['value'],
        "target": content.field_c07_items[i]['#paragraph'].field_c07_item_target.getValue()[0]['value'],
        "tag": "span",
        "class" : (content.field_c07_items[i]['#paragraph'].field_c07_item_active.getValue()[0]['value'] == '1') ? ' active':''
      }
  %}
  {% set navList = navList|merge([item]) %}
{% endfor %}

{% set data = {
    "title": "c07-on-page-jump-links",
    "navList": navList,
    "h1": {
      "type": "h2",
      "copy": content.field_c07_title[0]['#context']['value']
    },
    "desc": content.field_c07_description[0]['#context']['value']
  }
%}

{% include "@components/c07-on-page-jump-links/c07-on-page-jump-links.twig" 
  with {
    "data": data
  }
%}